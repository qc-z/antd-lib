{
  "name": "antd-packages",
  "version": "1.1.12",
  "description": "帮助你快速创建react组件库(集成antd)",
  "scripts": {
    "docs:dev": "pnpm gen-entry && dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "http-server docs-dist",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "pnpm gen-entry && father build && pnpm build:less",
    "build:less": "gulp --gulpfile ./scripts/gulpfile.js",
    "gen": "esno ./scripts/gen-component.ts --name",
    "gen-entry": "esno ./scripts/gen-entry.ts",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "create": "esno ./scripts/batch.ts",
    "dev": "father dev",
    "build:deps": "father prebundle",
    "getExports": "esno ./scripts/getExports.ts"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "unpkg": "dist/umd/antd-packages.min.js",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "keywords": [
    "ant",
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11",
    "not IE 10"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qc-z/antd-packages"
  },
  "homepage": "http://consolelog.cc",
  "author": "qc-z",
  "bugs": "https://github.com/qc-z/antd-packages/issues",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "antd": "^4.21.5"
  },
  "devDependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/pro-components": "^1.1.8",
    "@types/klaw-sync": "^6.0.1",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@umijs/fabric": "^2.12.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/ssr-darkreader": "^4.9.45",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitest/ui": "^0.18.1",
    "antd-img-crop": "^4.2.3",
    "array-move": "^4.0.0",
    "babel-plugin-import": "^1.13.5",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.1",
    "dumi": "^1.1.45",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "esno": "^0.16.3",
    "father": "4.0.0-rc.9",
    "fetch-jsonp": "^1.2.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^4.0.0",
    "gogocode": "^1.0.55",
    "gray-matter": "^4.0.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "happy-dom": "^6.0.4",
    "http-server": "^14.1.1",
    "immutability-helper": "^3.1.1",
    "jsdom": "^20.0.0",
    "klaw-sync": "^6.0.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pkg-exports": "^0.1.1",
    "plop": "2.7.4",
    "prettier": "^2.7.1",
    "qs": "^6.11.0",
    "rc-resize-observer": "^1.2.0",
    "rc-tween-one": "^3.0.6",
    "rc-virtual-list": "^3.4.8",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-fast-marquee": "^1.3.2",
    "react-helmet-async": "^1.3.0",
    "react-highlight-words": "^0.18.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-resizable": "^3.0.4",
    "react-sortable-hoc": "^2.0.0",
    "react-sticky": "^6.0.3",
    "react-test-renderer": "^18.2.0",
    "react-window": "^1.8.7",
    "vitest": "0.18.1",
    "yorkie": "^2.0.0"
  }
}
